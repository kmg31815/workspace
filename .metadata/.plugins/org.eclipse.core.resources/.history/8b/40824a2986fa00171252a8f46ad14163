package exam;

public class Bubble {

	public static void main(String[] args) {
		int n[] = { 57, 21, 36, 14, 42 };
		for (int i = 0; i < n.length - 1; i++) {
			for (int j = i + 1; j < n.length; j++) {
				if (n[j] < n[i]) {
					int tmp = n[i];
					n[i] = n[j];
					n[j] = tmp;
				}
			}
		}
	}

}
